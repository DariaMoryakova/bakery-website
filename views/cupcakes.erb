  <div class="container row" id="sortCakes">
<% @cupcakes.each do |cupcake| %>
      <div class="col s12 m7 l4" id="pistachio">
        <div class="card">
          <div class="card-image">
            <img src= '<%= cupcake.img %>'>
            <span class="card-title">Card Title</span>
          </div>
            <div class="card-action">
            <a href="#">This is a link</a>
          </div>
        </div>
      </div>


    <% end %>